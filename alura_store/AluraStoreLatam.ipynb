import matplotlib.pyplot as plt

def graficar_dispersi√≥n_geogr√°fica(df, nombre_tienda):
    """Gr√°fico de dispersi√≥n de latitud vs longitud."""
    plt.figure(figsize=(6, 5))
    plt.scatter(df["lon"], df["lat"], alpha=0.4, s=15, color="green")
    plt.title(f"Distribuci√≥n geogr√°fica de ventas ‚Äì {nombre_tienda}")
    plt.xlabel("Longitud")
    plt.ylabel("Latitud")
    plt.grid(True, linestyle="--", alpha=0.3)
    plt.show()


def graficar_heatmap(df, nombre_tienda):
    """Heatmap 2D utilizando histograma de densidad."""
    plt.figure(figsize=(6, 5))
    plt.hist2d(df["lon"], df["lat"], bins=40, cmap="YlGnBu")
    plt.colorbar(label="Densidad de ventas")
    plt.title(f"Mapa de calor ‚Äì {nombre_tienda}")
    plt.xlabel("Longitud")
    plt.ylabel("Latitud")
    plt.show()


